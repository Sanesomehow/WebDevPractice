<%- include("partials/header"); -%>

<h1>Welcome to the Journal Management System</h1>

<p class="intro"><%= homeStartingContent %></p>

<% posts.forEach(function(post) { %>
    <h1><%=  post.title %> </h1>
    <p>
        <%=    post.content.substring(0,100) + " ... " %>
    <a href="/posts/<%= post._id %>">Read More</a> 
</p>
<% }) %>


